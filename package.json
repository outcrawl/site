{
  "name": "outcrawl",
  "version": "1.0.0",
  "author": "tinrab <tin.rabzelj@gmail.com>",
  "scripts": {
    "dev": "yarn clean && gatsby develop -p 3000",
    "build": "yarn clean && gatsby build",
    "format": "prettier --write 'src/**/*.{js,scss}'",
    "clean": "del-cli .cache public",
    "deploy": "aws s3 cp public s3://outcrawl.com/ --recursive",
    "purge": "aws s3 rm s3://outcrawl.com/ --recursive --exclude \"static/*\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.5",
    "escape-html": "^1.0.3",
    "gatsby": "^1.9.255",
    "gatsby-link": "^1.6.41",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-google-fonts": "0.0.4",
    "gatsby-plugin-jss": "^1.5.14",
    "gatsby-plugin-manifest": "^1.0.21",
    "gatsby-plugin-purify-css": "^2.2.1",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-react-svg": "^1.1.1",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sharp": "^1.6.43",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-images": "^1.5.62",
    "gatsby-source-filesystem": "^1.5.34",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.23",
    "marked": "^0.3.19",
    "material-ui": "^1.0.0-beta.42",
    "md5": "^2.2.1",
    "normalize.css": "^8.0.0",
    "parse-numeric-range": "^0.0.2",
    "prismjs": "^1.14.0",
    "react-async-script": "^0.9.1",
    "react-google-recaptcha": "^0.11.0",
    "react-helmet": "^5.2.0",
    "slug": "^0.9.1",
    "stats-webpack-plugin": "^0.6.2"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "prettier": "^1.12.1"
  }
}
